package Abstract.example;

import java.util.Arrays;

public class Arrray {
private char arr[];

public Arrray() {
	
}
public void createArrray(int length) {
	arr= new char[length];
	for(int i=0;i<arr.length;i++) {
		arr[i]=(char)('A'+i);
	}
}
public char[] getArray() {
	return arr;
}
public boolean findElement(char item) {
	int i=Arrays.binarySearch(arr, item);
			if(i>=0) {
				return true;
			}
			else {
				return false;
			}
}
}
